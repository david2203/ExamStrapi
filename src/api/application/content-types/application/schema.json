{
  "kind": "collectionType",
  "collectionName": "applications",
  "info": {
    "singularName": "application",
    "pluralName": "applications",
    "displayName": "Application",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "applications"
    },
    "job": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::job.job",
      "inversedBy": "applications"
    },
    "applyEmail": {
      "type": "email"
    },
    "CV": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ]
    },
    "startDate": {
      "type": "date"
    }
  }
}
